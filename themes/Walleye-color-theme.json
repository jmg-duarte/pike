  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    // Functions / Methods
    "magicFunction": {
      "fontStyle": "italic"
    },
    // Function Parameters
    "*.controlFlow": {
      "fontStyle": "bold"
    },
    "*.unsafe": {
      "fontStyle": "bold"
    },
    "*.consuming": {
      "fontStyle": "italic"
    },
    "*.mutable": {
      "fontStyle": "underline"
    },
    // They're usually upper case
    "*.constant": {
      "fontStyle": ""
    },
    "builtinConstant": {
      "fontStyle": ""
    },
    "*.builtin": {
      "fontStyle": ""
    },
    "*.deprecated": {
      "fontStyle": "strikethrough"
    },
    "comment": "#6d6d6d",
    "label": {
      "fontStyle": "bold"
    }
  },
  "tokenColors": [
    {
      "name": "Control Flow",
      "scope": ["keyword.control.flow"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6d6d6d"
      }
    }
  ]
}
