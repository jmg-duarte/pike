  "semanticHighlighting": true,
  "semanticTokenColors": {
    // Functions / Methods
    "magicFunction": {
      "fontStyle": "italic"
    },
    // Function Parameters
    "*.controlFlow": {
      "fontStyle": "bold"
    },
    "*.unsafe": {
      "fontStyle": "bold"
    },
    "*.consuming": {
      "fontStyle": "italic"
    },
    "*.mutable": {
      "fontStyle": "underline"
    },
    // They're usually upper case
    "*.constant": {
      "fontStyle": ""
    },
    "builtinConstant": {
      "fontStyle": ""
    },
    "*.builtin": {
      "fontStyle": ""
    },
    "*.deprecated": {
      "fontStyle": "strikethrough"
    },
    // "comment": "#6d6d6d",
    "label": {
      "fontStyle": "bold"
    }
  },
